pipeline {
    agent any
    stages{
        stage('Init'){
            steps {
                echo "Initializing build..." 
            }
        }
        stage('Build'){
            steps {
                echo "Building application..." 
            }
        }
        stage('Deploy'){
            steps {
                echo "Deploying application..." 
                echo "Code Deployed." 
            }
        }
    }
}
